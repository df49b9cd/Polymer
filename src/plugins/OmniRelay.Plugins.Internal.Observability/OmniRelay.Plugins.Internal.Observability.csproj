<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsTrimmable>true</IsTrimmable>
    <EnableTrimAnalyzer>true</EnableTrimAnalyzer>
    <EnableAotAnalyzer>true</EnableAotAnalyzer>
    <EnableSingleFileAnalyzer>true</EnableSingleFileAnalyzer>
    <AssemblyName>OmniRelay.Plugins.Internal.Observability</AssemblyName>
    <RootNamespace>OmniRelay.Plugins.Internal.Observability</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../../OmniRelay.Diagnostics.Telemetry/OmniRelay.Diagnostics.Telemetry.csproj" />
    <ProjectReference Include="../../OmniRelay.Diagnostics.Logging/OmniRelay.Diagnostics.Logging.csproj" />
    <ProjectReference Include="../../OmniRelay.Diagnostics.Probes/OmniRelay.Diagnostics.Probes.csproj" />
    <ProjectReference Include="../../OmniRelay.Diagnostics.Alerting/OmniRelay.Diagnostics.Alerting.csproj" />
    <ProjectReference Include="../../OmniRelay.Diagnostics.Documentation/OmniRelay.Diagnostics.Documentation.csproj" />
    <ProjectReference Include="../../abstractions/OmniRelay.Observability.Abstractions/OmniRelay.Observability.Abstractions.csproj" />
  </ItemGroup>
</Project>
