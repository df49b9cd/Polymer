<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <PackageId>Polymer.Codegen.Protobuf</PackageId>
    <Description>protoc plug-in for generating Polymer dispatcher and client bindings.</Description>
    <PackageTags>polymer;protobuf;codegen;protoc</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Google.Protobuf" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Polymer.Codegen.Protobuf.Core\Polymer.Codegen.Protobuf.Core.csproj" />
  </ItemGroup>
</Project>
