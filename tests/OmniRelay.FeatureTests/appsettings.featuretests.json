{
  "omniRelay": {
    "service": "feature-tests-relay",
    "logging": {
      "level": "Debug",
      "overrides": {
        "System.Net.Http": "Warning"
      }
    },
    "diagnostics": {
      "openTelemetry": {
        "enabled": false,
        "enableMetrics": false,
        "prometheus": {
          "enabled": false
        },
        "otlp": {
          "enabled": false
        }
      }
    },
    "encodings": {
      "json": {
        "profiles": {
          "feature-tests": {
            "options": {
              "propertyNameCaseInsensitive": true,
              "writeIndented": true
            }
          }
        }
      }
    }
  }
}
