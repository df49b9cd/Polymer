{
  "omniRelay": {
    "service": "feature-tests-relay",
    "logging": {
      "level": "Debug",
      "overrides": {
        "System.Net.Http": "Warning"
      }
    },
    "inbounds": {
      "http": [
        {
          "name": "FeatureTestsDiagnostics",
          "urls": [
            "http://127.0.0.1:19180"
          ]
        }
      ]
    },
    "diagnostics": {
      "controlPlane": {
        "enablePeerDiagnostics": true
      },
      "openTelemetry": {
        "enabled": false,
        "enableMetrics": false,
        "prometheus": {
          "enabled": false
        },
        "otlp": {
          "enabled": false
        }
      }
    },
    "mesh": {
      "gossip": {
        "enabled": true,
        "nodeId": "feature-tests-dispatcher",
        "role": "dispatcher",
        "clusterId": "feature-tests",
        "region": "feature-tests-region",
        "meshVersion": "feature-tests",
        "bindAddress": "127.0.0.1",
        "advertiseHost": "127.0.0.1",
        "advertisePort": 29650,
        "port": 29650,
        "interval": "00:00:01",
        "suspicionInterval": "00:00:02",
        "pingTimeout": "00:00:00.5000000",
        "retransmitLimit": 2,
        "fanout": 2,
        "metadataRefreshPeriod": "00:00:02",
        "labels": {
          "rack": "rack-control"
        },
        "tls": {
          "allowUntrustedCertificates": true,
          "checkCertificateRevocation": false
        }
      }
    },
    "encodings": {
      "json": {
        "profiles": {
          "feature-tests": {
            "options": {
              "propertyNameCaseInsensitive": true,
              "writeIndented": true
            }
          }
        }
      }
    }
  }
}
